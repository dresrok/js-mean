{
  "name": "incident-desk-api",
  "version": "0.1.0",
  "description": "Clase 1 backend for Incident Desk training module.",
  "main": "dist/server.js",
  "scripts": {
    "dev": "tsx watch src/server.ts",
    "debug": "tsx --inspect-brk src/server.ts",
    "start": "node dist/server.js",
    "build": "tsc --project tsconfig.json",
    "lint": "eslint \"src/**/*.ts\" --max-warnings=0",
    "typecheck": "tsc --project tsconfig.json --noEmit",
    "seed": "tsx src/core/seeder.ts",
    "generate-key": "node -e \"console.log(require('node:crypto').randomBytes(64).toString('base64'))\""
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "express-rate-limit": "^8.2.1",
    "helmet": "^8.1.0",
    "jsonwebtoken": "^9.0.2",
    "lru-cache": "^11.2.2",
    "mongoose": "^7.6.0",
    "pino-http": "^11.0.0",
    "zod": "^4.1.13"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/compression": "^1.8.1",
    "@types/cors": "^2.8.19",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/node": "^20.12.7",
    "@typescript-eslint/eslint-plugin": "^8.46.4",
    "@typescript-eslint/parser": "^8.46.4",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "prettier": "^3.6.2",
    "ts-node": "^10.9.2",
    "tslib": "^2.6.2",
    "tsx": "^4.7.0",
    "typescript": "^5.4.5"
  }
}
